		suite.addTestSuite(IdentifyableSetTest.class);
		suite.addTestSuite(GroupTest.class);
		suite.addTestSuite(SportsClubTest.class);
		suite.addTestSuite(PersonRelationTest.class);
		suite.addTestSuite(PersonTest.class);
		suite.addTestSuite(FourTest.class);
		suite.addTestSuite(SeasonTest.class);
		suite.addTestSuite(ClubReportTest.class);
		suite.addTestSuite(ClubTest.class);
		suite.addTestSuite(TupleSetTest.class);
		suite.addTestSuite(OrganizationTest.class);
		//$JUnit-END$
